{% if is_granted(authorized_role|default('ROLE_SUPER_ADMIN')) %}
	<div class="eSeeraiwer-toolbar-block" style="float:right;">
	    <div class="eSeeraiwer-toolbar-icon">
	        <a onclick="toggleInlineTransStatus();" title="Activate/Desactivate inline translation." style="cursor:pointer;">
	            <img id="inlineTransImgGrey" alt="inline-translation" src="data:image/png;base64,">
	            <img style="display:none;" id="inlineTransImgGreen" alt="inline-translation" src="data:image/png;base64,">
	        </a>
	    </div>
	</div>
	<div class="eSeeraiwer-toolbar-block" style="float:right;">
	    <div class="eSeeraiwer-toolbar-icon">
	        <a href="{{ path('jms_translation_index') }}">
	            <img width="0" height="28" alt="" src="">
	            <span>Translation</span>
	        </a>
	    </div>
	</div>
	<script>
		function toggleInlineTransStatus()
		{
			inlineTransStatus = window.localStorage.getItem('inlineTransStatus');
			if(inlineTransStatus == 0)
			{
				$('#inlineTransImgGrey').hide();
				$('#inlineTransImgGreen').show();
				$('a').click(function(){ return false; });
				window.localStorage.setItem('inlineTransStatus', 1);
			}
			else if(inlineTransStatus == 1)
			{
				$('#inlineTransImgGrey').show();
				$('#inlineTransImgGreen').hide();
				window.localStorage.setItem('inlineTransStatus', 0);
				window.location.reload();
			}
		}
	</script>
{% endif %}